{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "createdAt": {"type": "string", "format": "date-time"},
        "data": {
            "type": "object",
            "properties": {
                "CPU model": {"type": "string"},
                "Hard disk size": {"type": "string"},
                "color": {"type": "string"},
                "price": {"type": "number"}
            },
            "required": ["price"]
        },
        "id": {"type": "string"},
        "name": {"type": "string"}
    },
    "required": ["createdAt", "data", "id", "name"]
}